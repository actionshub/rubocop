{
  "problemMatcher": [
    {
      "owner": "rubocop-fatal",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^\\s].*):(\\d+):(\\d+)\\:\\s+E:\\s+([^\\\\s].*):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
